{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2015",
		"lib":[
			"es2015"
		],
		"outDir": "dist",
		"declaration": true,
		"sourceMap": false,
		"newLine": "LF",
		"removeComments": true,

		"noImplicitAny": true,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"strictNullChecks": true,
		"alwaysStrict": true
	}
}
